{
  "name": "monocle",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "paths": {
    "@/*": [
      "./content/*"
    ]
  },
  "scripts": {
    "dev": "extension dev .",
    "dev:firefox": "extension dev . --browser=firefox",
    "build": "npm run tsc && extension build .",
    "build:zip": "npm run tsc && extension build --zip .",
    "build:firefox": "npm run tsc && extension build . --browser=firefox",
    "build:firefox:zip": "npm run tsc && extension build . --browser=firefox --zip",
    "preview": "npm run tsc && extension preview .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc --noEmit",
    "fmt": "biome check --write --unsafe",
    "fmt:check": "biome check"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "A browser extension that adds a command palette interface to any webpage",
  "dependencies": {
    "@radix-ui/react-popover": "^1.1.11",
    "@reduxjs/toolkit": "^2.9.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-select": "^5.10.2",
    "ts-pattern": "^5.8.0",
    "usehooks-ts": "^3.1.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.0.13",
    "@types/react-dom": "^19.1.9",
    "@types/webpack-env": "^1.18.8",
    "extension": "^2.1.2",
    "tailwindcss": "^4.1.13",
    "typescript": "5.9.2"
  }
}
